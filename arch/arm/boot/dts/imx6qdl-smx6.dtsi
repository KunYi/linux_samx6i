};

#define smarc_ser2 uart4
&uart4 {
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_smx6_uart4>;
        fsl,uart-has-rtscts;
};

#define smarc_ser3 uart5
&uart5 {
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_smx6_uart5>;
};

&fec {
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_smx6_enet_fec>;
