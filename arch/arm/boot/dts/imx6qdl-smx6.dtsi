&pcie {
        wake-up-gpio = <&gpio6 18 0>;
#ifdef SMX6_PCIE_SWITCH
        reset-gpio = <&gpio2 0 GPIO_ACTIVE_LOW>,
                     <&gpio3 13 GPIO_ACTIVE_LOW>,
                     <&gpio3 14 GPIO_ACTIVE_LOW>,
                     <&gpio3 15 GPIO_ACTIVE_LOW>;
        no-gen2;
#else
        reset-gpio = <&gpio3 13 GPIO_ACTIVE_LOW>;
